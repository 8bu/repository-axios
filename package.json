{
  "name": "@8bu/axios",
  "version": "0.0.1",
  "packageManager": "pnpm@6.32.3",
  "description": "Axios with repository pattern",
  "keywords": [
    "axios",
    "repository"
  ],
  "license": "MIT",
  "author": "8bu <nhvlongg@gmail.com>",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "axios": "^0.26.1",
    "json-bigint": "^1.0.0",
    "qs": "^6.10.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.20.2",
    "@antfu/ni": "^0.14.0",
    "@babel/types": "^7.17.0",
    "@types/json-bigint": "^1.0.1",
    "@types/node": "^17.0.23",
    "@types/qs": "^6.9.7",
    "axios": "^0.26.1",
    "bumpp": "^7.1.1",
    "eslint": "^8.13.0",
    "esno": "^0.14.1",
    "json-bigint": "^1.0.0",
    "pnpm": "^6.32.6",
    "qs": "^6.10.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.2",
    "vite": "^2.9.1",
    "vitest": "^0.9.3"
  }
}
